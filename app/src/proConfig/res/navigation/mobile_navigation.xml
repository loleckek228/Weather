<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_search_city">


    <fragment
        android:id="@+id/navigation_search_city"
        android:name="com.geekbrains.android.homework.fragments.SearchCityFragment"
        android:label="@string/title_search_city"
        tools:layout="@layout/fragment_search_city" />

    <fragment
        android:id="@+id/navigation_weather"
        android:name="com.geekbrains.android.homework.fragments.WeatherFragment"
        android:label="@string/title_weather"
        tools:layout="@layout/fragment_weather" />

    <fragment
        android:id="@+id/navigation_maps"
        android:name="com.geekbrains.android.homework.fragments.MapFragment"
        android:label="@string/title_maps"
        tools:layout="@layout/activity_maps" />

    <fragment
        android:id="@+id/navigation_developer"
        android:name="com.geekbrains.android.homework.fragments.developer.DeveloperFragment"
        android:label="@string/title_developer"
        tools:layout="@layout/fragment_developer" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.geekbrains.android.homework.fragments.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/navigation_auth_google"
        android:name="com.geekbrains.android.homework.fragments.AuthGoogleFragment"
        android:label="@string/title_auth_google"
        tools:layout="@layout/fragment_auth_google" />

</navigation>
